   "source": [
    "def removeConsecutiveDuplicates(string):\n",
    "    ans = ''\n",
    "    for i in string:\n",
    "        if ans =='' or i != ans[len(ans)-1]:\n",
    "            ans += i\n",
    "    return ans\n",
    "\n",
    "a = removeConsecutiveDuplicates('aaabbc')\n",
    "print(a)\n",
    "     "
   ]
